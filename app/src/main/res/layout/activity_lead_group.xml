<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_lead_group"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.yt.wia.application.LeadGroupActivity">
    <include
        layout="@layout/title_bar"/>

    <LinearLayout
        android:id="@+id/lin1"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:orientation="horizontal">
        <Button
            android:id="@+id/hightnew"
            style="@style/MyBtn"
            android:text="高新技术企业数量"
            />
        <Button
            android:id="@+id/unitarea"
            style="@style/MyBtn"
            android:text="单位面积情况税收情况"
            />
        <Button
            android:id="@+id/btn_treed"
            style="@style/MyBtn0"
            />
        <Button
            style="@style/MyBtn0" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:orientation="horizontal">
        <Button
            style="@style/MyBtn0"
            />
        <Button
            style="@style/MyBtn0"
            />
        <Button
            style="@style/MyBtn0"
            />
        <Button
            style="@style/MyBtn0"
            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:layout_weight="1"
        android:layout_marginBottom="20dp">
        <Button
            style="@style/MyBtn0"
            />
        <Button
            style="@style/MyBtn0"
            />
        <Button
            style="@style/MyBtn0"
            />
        <Button
            style="@style/MyBtn0"
            />
    </LinearLayout>
</LinearLayout>
